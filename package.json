{
  "name": "memowiki",
  "version": "1.0.4",
  "description": "A persistent memory layer for your codebase + AI agent",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "workspaces": [
    "dashboard"
  ],
  "bin": {
    "memowiki": "./dist/index.js"
  },
  "files": [
    "dist/**",
    "dashboard/dist/dashboard/browser/**",
    "README.md",
    "LICENSE",
    "docs/**"
  ],
  "scripts": {
    "build": "tsc",
    "build:dashboard": "cd dashboard && npm run build",
    "build:all": "npm run build && npm run build:dashboard",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:serve": "nodemon --watch src --ext ts,js,json --ignore dashboard/dist --ignore .codewiki --signal SIGINT --exec \"ts-node src/index.ts serve\"",
    "test": "jest",
    "prepublishOnly": "npm run build:all",
    "publish": "npm version patch && npm publish --access public"
  },
  "keywords": [
    "ai",
    "wiki",
    "documentation",
    "memory"
  ],
  "author": "spanexx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spanexx/MemoWikiV1.git"
  },
  "homepage": "https://github.com/spanexx/MemoWikiV1#readme",
  "bugs": {
    "url": "https://github.com/spanexx/MemoWikiV1/issues"
  },
  "documentation": "https://spanexx.github.io/MemoWikiV1",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.7.9",
    "chalk": "^4.1.2",
    "chromadb": "^3.1.6",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.2.1",
    "openai": "^4.77.0",
    "simple-git": "^3.22.0",
    "ts-morph": "^21.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.15",
    "typescript": "^5.3.3"
  }
}
